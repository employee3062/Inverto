{
  "name": "inverto",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --minify-whitespace --minify-syntax --target bun --outfile dist/handler.js ./src/index.ts",
    "deploy": "./deploy.sh",
    "destroy": "aws cloudformation delete-stack --stack-name inverto-stack",
    "zip": "cd dist && zip handler.zip handler.js"
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.3.1",
    "elysia": "latest"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "module": "src/index.js"
}
